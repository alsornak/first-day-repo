{{#each doctors as |doctor| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      {{{get_emoji}}}
      <a href="/api/doctors/{{doctor.doctors_id}}">{{doctor.first_name}} {{doctor.last_name}}</a>
    </h2>
  </div>
  <div class="col-md-7">
    <p>
      {{doctor.speciality}}
    </p>
  </div>
</div>
{{/each}}
